<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.14.xsd">

    <changeSet id="002-insert-initial-journal-types" author="engineeringdigest">
        <insert tableName="journal_type" schemaName="${schemaName}">
            <column name="name" value="Personal"/>
        </insert>
        <insert tableName="journal_type" schemaName="${schemaName}">
            <column name="name" value="Work"/>
        </insert>
        <insert tableName="journal_type" schemaName="${schemaName}">
            <column name="name" value="Study"/>
        </insert>
        <rollback>
            <delete tableName="journal_type" schemaName="${schemaName}">
                <where>name='Draft'</where>
            </delete>
        </rollback>
    </changeSet>

</databaseChangeLog>
